lista_nomes =['Manuel', 'Laura', 'Antonio', 'Jasmim', 'Maria', 'Silvia', 'Lu', 'PancrÃ¡cio', 'Diogo', 'Ricardo', 'Miguel', 'Andre']
n = len(lista_nomes)
for k in range (n-1):
    for i in range  (n-k-1):
        if lista_nomes [i] > lista_nomes [i+1]:
            a = lista_nomes [i]
            lista_nomes[i] =lista_nomes[i+1]
            lista_nomes[i+1]=a
            print("Ordenou ",n-k,"trocou",lista_nomes[i],lista_nomes[i+1],"ordem",lista_nomes)
        print("ordem ",lista_nomes)
